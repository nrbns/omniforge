{
  "name": "omniforge",
  "version": "0.1.0",
  "private": true,
  "description": "World's first open-source, end-to-end Idea â†’ App â†’ Deployment â†’ App Store AI Builder",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:demo": "cross-env DEMO_MODE=true turbo run dev --filter=@omniforge/backend --filter=@omniforge/frontend",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:coverage": "turbo run test:coverage",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "type-check": "turbo run type-check",
    "db:migrate": "cd apps/backend && npx prisma migrate dev",
    "db:generate": "cd apps/backend && npx prisma generate",
    "db:studio": "cd apps/backend && npx prisma studio",
    "db:seed": "cd apps/backend && ts-node prisma/seed.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v",
    "demo": "npm run docker:up && npm run dev:demo",
    "setup": "npm install && npm run docker:up && npm run db:generate && npm run db:migrate && npm run db:seed",
    "clean": "turbo run clean && rm -rf node_modules",
    "prepare": "husky install || true"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "cross-env": "^7.0.3",
    "husky": "^8.0.3",
    "prettier": "^3.1.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "packageManager": "npm@9.8.1"
}
